{
  "name": "netlify-plugin-hashfiles",
  "description": "Netlify build plugin to get optimal file caching with hashed file names and immutable cache headers",
  "repository": "git://github.com/munter/netlify-plugin-hashfiles.git",
  "bugs": {
    "url": "https://github.com/munter/netlify-plugin-hashfiles/issues"
  },
  "version": "4.0.2",
  "license": "BSD-3-Clause",
  "maintainers": [
    {
      "name": "Peter MÃ¼ller",
      "email": "munter@fumle.dk"
    }
  ],
  "files": [
    "manifest.yml",
    "lib"
  ],
  "keywords": [
    "netlify",
    "netlify-plugin",
    "rev",
    "revision",
    "hash",
    "versioning",
    "cache",
    "expire",
    "static",
    "asset",
    "assets",
    "cdn"
  ],
  "dependencies": {
    "assetgraph": "6.6.0",
    "assetgraph-hashfiles": "1.1.1",
    "fs-extra": "9.1.0"
  },
  "devDependencies": {
    "@netlify/build": "5.2.0",
    "coveralls": "3.1.0",
    "cpy-cli": "3.1.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "magicpen-prism": "4.1.1",
    "mocha": "8.4.0",
    "node-sass": "5.0.0",
    "nyc": "15.1.0",
    "offline-github-changelog": "1.6.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "strip-ansi": "6.0.0",
    "typescript": "4.0.5",
    "unexpected": "11.15.0",
    "unexpected-assetgraph": "1.1.2",
    "unexpected-set": "3.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cpy site dist",
    "lint": "eslint .",
    "test": "mocha",
    "test:ci": "npm run netlify-build && npm run coverage",
    "netlify-build": "netlify-build",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text --all -- mocha --reporter dot && echo google-chrome coverage/lcov-report/index.html",
    "preversion": "offline-github-changelog --next=${npm_package_version} > CHANGELOG.md && git add CHANGELOG.md"
  },
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
}
